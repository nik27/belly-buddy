{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["prettier", "standard", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier", "import", "standard"],
  "rules": {
    "prettier/prettier": "error"
  }
}
